	</LinkControl>

	<!-- PLAYER 1 STORE -->
	<LinkControl player="1" press="true">
		<name>LinkPurchaseItem1Cmd</name>
		<key>NumPad1</key>
	</LinkControl>

	<LinkControl player="1" press="true">
		<name>LinkPurchaseItem1Cmd</name>
