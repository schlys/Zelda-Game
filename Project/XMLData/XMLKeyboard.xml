<?xml version="1.0" encoding="utf-8" ?>
<Controllers>
	<!-- GAME COMMANDS -->
	<GameControl>
		<name>GameEndCmd</name>
		<key>Q</key>
	</GameControl>

	<GameControl>
		<name>GameRestartCmd</name>
		<key>R</key>
	</GameControl>

	<GameControl>
		<name>GamePauseCmd</name>
		<key>Space</key>
	</GameControl>

	<GameControl>
		<name>GameItemSelectCmd</name>
		<key>I</key>
	</GameControl>

	<GameControl>
		<name>GameStartCmd</name>
		<key>X</key>
	</GameControl>

	<GameControl>
		<name>GameExitStoreCmd</name>
		<key>X</key>
	</GameControl>

	<!-- SET LINK COUNT -->
	<GameControl>
		<name>GameSetLinkCount1Cmd</name>
		<key>NumPad1</key>
	</GameControl>

	<GameControl>
		<name>GameSetLinkCount1Cmd</name>
		<key>D1</key>
	</GameControl>

	<GameControl>
		<name>GameSetLinkCount2Cmd</name>
		<key>NumPad2</key>
	</GameControl>

	<GameControl>
		<name>GameSetLinkCount2Cmd</name>
		<key>D2</key>
	</GameControl>
	
	<!-- PLAYER 1 MOVEMENT -->
	
	<LinkControl player="1">
		<name>LinkMoveUpCmd</name>
		<key>W</key>	
	</LinkControl>

	<LinkControl player="1">
		<name>LinkMoveDownCmd</name>
		<key>S</key>	
	</LinkControl>

	<LinkControl player="1">
		<name>LinkMoveRightCmd</name>
		<key>D</key>	
	</LinkControl>

	<LinkControl player="1">
		<name>LinkMoveLeftCmd</name>
		<key>A</key>
	</LinkControl>
	
	<!--Player 2 Movement-->
	
	<LinkControl player="2">
		<name>LinkMoveUpCmd</name>
		<key>Up</key>
	</LinkControl>

	<LinkControl player="2">
		<name>LinkMoveDownCmd</name>
		<key>Down</key>
	</LinkControl>
w
	<LinkControl player="2">
		<name>LinkMoveRightCmd</name>
		<key>Right</key>
	</LinkControl>

	<LinkControl player="2">
		<name>LinkMoveLeftCmd</name>
		<key>Left</key>
	</LinkControl>

	<!-- PLAYER 1 ITEM SELECTION -->
	<LinkControl player="1" press="true">
		<name>LinkItemSelectRightCmd</name>
		<key>Right</key>
	</LinkControl>
	
	<LinkControl player="1" press="true">
		<name>LinkItemSelectLeftCmd</name>
		<key>Left</key>
	</LinkControl>

	<LinkControl player="1" press="true">
		<name>LinkItemSelectUpCmd</name>
		<key>Up</key>
	</LinkControl>

	<LinkControl player="1" press="true">
		<name>LinkItemSelectDownCmd</name>
		<key>Down</key>
	</LinkControl>

	<LinkControl player="1" press="true">
		<name>LinkSelectItem1Cmd</name>
		<key>D1</key>
	</LinkControl>

	<LinkControl player="1" press="true">
		<name>LinkSelectItem1Cmd</name>
		<key>NumPad1</key>
	</LinkControl>

	<LinkControl player="1" press="true">
		<name>LinkSelectItem2Cmd</name>
		<key>D2</key>
	</LinkControl>

	<LinkControl player="1" press="true">
		<name>LinkSelectItem2Cmd</name>
		<key>NumPad2</key>
	</LinkControl>

	<LinkControl player="1" press="true">
		<name>LinkSelectItemCmd</name>
